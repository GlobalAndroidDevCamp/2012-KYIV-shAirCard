<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GetAllEvents</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.GetAllEvents</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddEvent</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.AddEvent</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateEvent</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.UpdateEvent</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetFilteredEvents</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.GetFilteredEvents</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetClosestEvent</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.GetClosestEvent</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddVCard</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.AddVCard</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetAllVCardsByEventId</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.GetAllVCardsByEventId</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetCompleteCard</servlet-name>
		<servlet-class>com.gtug.shaircard.servlet.GetCompleteCard</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetAllEvents</servlet-name>
		<url-pattern>/get_all_events</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddEvent</servlet-name>
		<url-pattern>/add_event</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateEvent</servlet-name>
		<url-pattern>/update_event</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetFilteredEvents</servlet-name>
		<url-pattern>/get_filtered_events</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetClosestEvent</servlet-name>
		<url-pattern>/get_closest_event</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddVCard</servlet-name>
		<url-pattern>/add_vcard</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetAllVCardsByEventId</servlet-name>
		<url-pattern>/get_all_vcards_by_eventid</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetCompleteCard</servlet-name>
		<url-pattern>/get_complete_card</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
